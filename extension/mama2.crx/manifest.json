{
  "name": "MAMA2",
  "version": "2.0",
  "manifest_version": 2,
  "permissions": ["MAMA2"],
  "browser_action": {
    "default_icon": "icon64.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["tabs","notifications","http://*/*","https://*/*"]
}
